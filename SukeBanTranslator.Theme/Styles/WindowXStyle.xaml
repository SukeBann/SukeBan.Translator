<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:pucore="clr-namespace:Panuon.UI.Core;assembly=Panuon.UI.Core"
                    xmlns:pu="clr-namespace:Panuon.UI.Silver;assembly=Panuon.UI.Silver">
    <ResourceDictionary.MergedDictionaries>
        <pucore:SharedResourceDictionary Source="pack://application:,,,/Panuon.UI.Silver;component/Control.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style x:Key="ShellWindowXStyle"
           TargetType="pu:WindowX"
           BasedOn="{StaticResource {x:Type pu:WindowX}}">
        <Setter Property="Foreground"
                Value="{DynamicResource ThemeBrush_100}" />
        <Setter Property="Title"
                Value="SukeBan'Translator" />
    </Style>
</ResourceDictionary>